{
    "compilerOptions": {
        "baseUrl": ".",
        "esModuleInterop": true,
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "h",
        "lib": ["dom", "esnext"],
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "target": "es5",
        "noImplicitAny": true
